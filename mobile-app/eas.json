{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "release": {
        "env": {
          "reactNativeArchitectures": "arm64-v8a"
        },
        "android": {
          "buildType": "apk",
          "gradleCommand": ":app:assembleRelease"
        }
      }
  },
  "submit": {
    "production": {}
  }
}
